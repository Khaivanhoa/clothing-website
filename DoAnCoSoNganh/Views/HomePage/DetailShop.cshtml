                <script>
                    document.querySelectorAll('.toggle-header').forEach(header => {
                        header.addEventListener('click', function () {
                            const content = this.nextElementSibling; // Lấy phần tử <ul> kế tiếp
                            const toggleIcon = this.querySelector('.toggle-icon');

                            // Kiểm tra xem nội dung có đang hiển thị không
                            const isVisible = content.style.maxHeight;

                            if (!isVisible || isVisible === '0px') {
                                content.style.maxHeight = content.scrollHeight + 'px'; // Thiết lập chiều cao tối đa để hiện nội dung
                                toggleIcon.textContent = '-'; // Thay đổi biểu tượng thành dấu trừ
                            } else {
                                content.style.maxHeight = '0'; // Thiết lập chiều cao tối đa là 0 để ẩn nội dung
                                toggleIcon.textContent = '+'; // Thay đổi biểu tượng thành dấu cộng
                            }
                        });
                    });
                </script>

                <script>
                    const filterButton = document.getElementById('filterButton');
                    const filterMenu = document.getElementById('filterMenu');
                    const closeFilter = document.getElementById('closeFilter');
                    const overlay = document.getElementById('overlay');

                    // Mở menu bộ lọc và hiển thị overlay
                    filterButton.addEventListener('click', function () {
                        filterMenu.style.right = '0'; // Đưa menu vào màn hình
                        overlay.style.display = 'block'; // Hiển thị overlay
                    });

                    // Đóng menu bộ lọc và ẩn overlay
                    closeFilter.addEventListener('click', function () {
                        filterMenu.style.right = '-400px'; // Đưa menu ra ngoài màn hình
                        overlay.style.display = 'none'; // Ẩn overlay
                    });

                    // Đóng menu khi click ra ngoài
                    window.addEventListener('click', function (event) {
                        if (!filterMenu.contains(event.target) && !filterButton.contains(event.target)) {
                            filterMenu.style.right = '-400px'; // Đưa menu ra ngoài màn hình
                            overlay.style.display = 'none'; // Ẩn overlay
                        }
                    });

                </script>

